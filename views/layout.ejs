        <script type="text/javascript">
        $('#rd_Living label').click(function () {
            var livingval = $(this).siblings('input').val();
            if (livingval == 'Fulltime') {
                $('.timeslotdiv').hide();
            }
            else if (livingval == 'Parttime') {
                $('.timeslotdiv').show();
            }
        });

        $(".timeslotselection").click(function () {
            var rdid = $(this).children("input[type='radio']").attr("id");
            document.getElementById(rdid).checked = true;
            $(".timeslotselection.active").removeClass("active");
            $(this).addClass("active");
        });

        $('#chk_RequirementType input').click(function () {
            var btn_Radio = $(this).attr('id');
            var itemChecked = $("#" + btn_Radio).is(":checked");

            if (btn_Radio == 'chk_RequirementType_0') {
                $('#Container_Services').slideToggle();

                $("#Container_Services input[type='checkbox']").each(function () {
                    var ischeckflag = $(this).prop('checked');
                    if (ischeckflag == true) {
                        $(this).click();
                    }

                })
            }
            else if (btn_Radio == 'chk_RequirementType_1') {
                $('#Container_Food').slideToggle();

                $("#Container_Food input[type='checkbox']").each(function () {
                    var ischeckflag = $(this).prop('checked');
                    if (ischeckflag == true) {
                        $(this).click();
                    }

                })
            }
            else if (btn_Radio == 'chk_RequirementType_2') {
                $('#Container_Baby').slideToggle();

                $("#Container_Baby input[type='checkbox']").each(function () {
                    var ischeckflag = $(this).prop('checked');
                    if (ischeckflag == true) {
                        $(this).click();
                    }

                })
            }
            else if (btn_Radio == 'chk_RequirementType_3') {
                $('#Container_Elderly').slideToggle();

                $("#Container_Elderly input[type='checkbox']").prop('checked', false);
            }

            fn_checkBlocks();
        });

        function fn_checkBlocks() {

            $("#Container_Members").hide();
            $("#Container_Rooms").hide();

            var itemChecked0 = $("#chk_RequirementType_0").is(":checked");
            var itemChecked1 = $("#chk_RequirementType_1").is(":checked");

            if (itemChecked0 == true) {
                $("#Container_Members").fadeIn();
                $("#Container_Rooms").fadeIn();
            }
            else if (itemChecked1 == true) {
                $("#Container_Members").fadeIn();
            }
        }

        function fn_RequirementType(sender, args) {

            var isValid = false;

            $("#chk_RequirementType input").each(function () {

                if ($(this).is(":checked")) {

                    isValid = true;

                }

            });

            args.IsValid = isValid;
        }

        function fn_RequirementHours(sender, args) {

            var isValid = false;

            if ($("#rd_4Hours").is(":checked")) {
                isValid = true;
            }

            else if ($("#rd_8Hours").is(":checked")) {
                isValid = true;
            }

            else if ($("#rd_10Hours").is(":checked")) {
                isValid = true;
            }

            else if ($("#rd_24Hours").is(":checked")) {
                isValid = true;
            }

            args.IsValid = isValid;
        }
    </script>
